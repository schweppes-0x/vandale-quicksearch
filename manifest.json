{
  "manifest_version": 3,
  "name": "VanDale Quick Search",
  "version": "1.1.0",
  "description": "QuickSearch the VanDale dictionary for a word",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo-16.png",
      "48": "logo-48.png",
      "128": "logo-128.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "vandale-quicksearch-tonydalov",
      "strict_min_version": "42.0"
    }
  }
}